{
  "compilerOptions": {
    "strictNullChecks": false,
    "noImplicitAny": false,
    "module": "commonJS",
    "strict": true,
    "baseUrl": "./",
    "outDir": "build",
    "removeComments": true,
    "experimentalDecorators": true,
    "target": "ES2017",
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "sourceMap": true,
    "diagnostics": true,
    "extendedDiagnostics": true,
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "./*.ts"
  ],
  "exclude": [
    "*node_modules*"
  ],
  
}